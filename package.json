{
  "name": "optic-release-automation-action",
  "version": "4.12.3",
  "description": "Automate the release process of your npm modules by providing them OTP on-demand.",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "node --test --experimental-test-module-mocks  --experimental-test-coverage --test-coverage-exclude=test/** --test-coverage-branches=100 --test-coverage-lines=100 --test-coverage-functions=100",
    "build": "ncc build src/index.js",
    "prepare": "husky",
    "update-readme-TOC": "doctoc README.md"
  },
  "keywords": [],
  "author": {
    "name": "Salman Mitha",
    "email": "salman.mitha@nearform.com"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.2",
    "@actions/exec": "^2.0.0",
    "@actions/github": "^7.0.0",
    "adm-zip": "^0.5.16",
    "conventional-changelog-monorepo": "github:nearform/conventional-changelog",
    "fastify": "^5.7.3",
    "lodash.isequal": "^4.5.0",
    "lodash.template": "^4.5.0",
    "lodash.truncate": "^4.4.2",
    "lodash.uniqwith": "^4.5.0",
    "markdown-it": "^14.1.1",
    "node-fetch": "^2.6.6",
    "p-map": "^7.0.4",
    "semver": "^7.7.3",
    "string-format": "^2.0.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.4",
    "doctoc": "^2.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.1.7",
    "lodash.clonedeep": "^4.5.0",
    "prettier": "^2.8.8",
    "sinon": "^21.0.1"
  }
}
